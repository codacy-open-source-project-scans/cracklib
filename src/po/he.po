#: lib/fascist.c:516
msgid "it is based on your username"
msgstr "מבוססת על שם המשתמש שלך"

#: lib/fascist.c:576
msgid "it is based upon your password entry"
msgstr "מבוססת על הקלדת הססמה שלך"

#: lib/fascist.c:593 lib/fascist.c:601
msgid "it is derived from your password entry"
msgstr "נגזרת מהקלדת הססמה שלך"

#: lib/fascist.c:613
msgid "it is derivable from your password entry"
msgstr "ניתן לגזור אותה מהקלדת הססמה שלך"

#: lib/fascist.c:625
msgid "it's derivable from your password entry"
msgstr "ניתן לגזור אותה מהקלדת הססמה שלך"

#: lib/fascist.c:683
msgid "you are not registered in the password file"
msgstr "לא נרשמת בקובץ הססמאות"

#: lib/fascist.c:718
msgid "it is WAY too short"
msgstr "קצרה ביותר"

#: lib/fascist.c:723
msgid "it is too short"
msgstr "קצרה מדי"

#: lib/fascist.c:740
msgid "it does not contain enough DIFFERENT characters"
msgstr "אינה מכילה מספיק תווים שונים"

#: lib/fascist.c:754
msgid "it is all whitespace"
msgstr "כולה רווחים"

#: lib/fascist.c:773
msgid "it is too simplistic/systematic"
msgstr "מערכתית/פשוטה מדי"

#: lib/fascist.c:778
msgid "it looks like a National Insurance number."
msgstr "נראית כמו מספר תעודת זהות/ביטוח לאומי."

#: lib/fascist.c:810
msgid "it is based on a dictionary word"
msgstr "מבוססת על מילה במילון"

#: lib/fascist.c:829
msgid "it is based on a (reversed) dictionary word"
msgstr "מבוססת על (היפוך של) מילה במילון"

#: lib/fascist.c:874
msgid "error loading dictionary"
msgstr "שגיאה בטעינת המילון"
